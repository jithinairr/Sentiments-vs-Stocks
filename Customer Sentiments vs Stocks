    "import matplotlib.pyplot as plt\n",
    "\n",
    "customer_complaints_filtered = customer_complaints[customer_complaints['State'] != \"UNKNOWN\"]\n",
    "# Group the data by year and state and count the number of issues\n",
    "grouped_data = customer_complaints_filtered.groupby(['Year', 'State'])['Issue'].count().reset_index()\n",
    "\n",
    "# Pivot the data to make it suitable for plotting\n",
    "pivot_data = grouped_data.pivot(index='State', columns='Year', values='Issue')\n",
    "\n",
    "# Plot the bar plot\n",
    "ax = pivot_data.plot(kind='bar', stacked=True, figsize=(10, 6))\n",
    "ax.set_title('Customer complaints per State by Year in USA')\n",
    "ax.set_xlabel('State')\n",
    "ax.set_ylabel('Total number of complaints')\n",
    "plt.legend(title='Year')\n",
    "plt.show()"
   ]
  },
