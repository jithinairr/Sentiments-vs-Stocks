   "cell_type": "code",
   "execution_count": 40,
   "id": "148f8d08",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "Mean Squared Error: 568.8371154495264\n",
      "R-squared: 0.0016739774407652508\n"
     ]
